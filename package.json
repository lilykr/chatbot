{
	"name": "chatbot",
	"version": "1.0.0",
	"main": "index.ts",
	"scripts": {
		"postinstall": "patch-package",
		"start": "expo start --dev-client",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"lint": "biome check . --formatter-enabled=true --linter-enabled=true --organize-imports-enabled=true --diagnostic-level error --write",
		"typecheck": "tsc --project tsconfig.json --noEmit --pretty",
		"prebuild": "expo prebuild --clean",
		"e2e": "maestro test maestro/chat.yaml"
	},
	"dependencies": {
		"@expo-google-fonts/epilogue": "^0.2.3",
		"@expo/metro-runtime": "~4.0.1",
		"expo": "~52.0.37",
		"expo-av": "~15.0.2",
		"expo-camera": "~16.0.17",
		"expo-font": "~13.0.4",
		"expo-linear-gradient": "~14.0.2",
		"expo-screen-orientation": "~8.0.4",
		"expo-splash-screen": "~0.29.22",
		"expo-status-bar": "~2.0.1",
		"expo-video": "~2.0.5",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-native": "0.76.7",
		"react-native-get-random-values": "~1.11.0",
		"react-native-gifted-chat": "2.6.5",
		"react-native-reanimated": "~3.16.1",
		"react-native-safe-area-context": "4.12.0",
		"react-native-web": "~0.19.13"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@biomejs/biome": "1.9.4",
		"@types/react": "~18.3.12",
		"patch-package": "^8.0.0",
		"typescript": "^5.3.3"
	},
	"private": true
}
