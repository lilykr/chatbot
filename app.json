{
	"expo": {
		"name": "lisa-lou",
		"slug": "lisa-lou",
		"scheme": "lilykr.lisalou",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"newArchEnabled": true,
		"experiments": {
			"typedRoutes": true
		},
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.lilykr.lisalou",
			"backgroundColor": "#080607",
			"infoPlist": {
				"NSSpeechRecognitionUsageDescription": "Allow $(PRODUCT_NAME) to use speech recognition.",
				"NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to use the microphone.",
				"ITSAppUsesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#080607"
			},
			"package": "com.lilykr.lisalou",
			"permissions": [
				"android.permission.CAMERA",
				"android.permission.RECORD_AUDIO",
				"android.permission.MODIFY_AUDIO_SETTINGS"
			]
		},
		"androidStatusBar": {
			"translucent": true,
			"backgroundColor": "#090607"
		},
		"androidNavigationBar": {
			"barStyle": "dark-content",
			"backgroundColor": "#090607"
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"bundler": "metro",
			"output": "server"
		},
		"plugins": [
			[
				"expo-router",
				{
					"origin": "https://lisa-lou.expo.app"
				}
			],
			["expo-localization"],
			[
				"expo-camera",
				{
					"cameraPermission": "Autoriser Lisa-Lou à accéder à votre caméra",
					"microphonePermission": "Autoriser Lisa-Lou à accéder à votre microphone",
					"recordAudioAndroid": true
				}
			],
			[
				"expo-video",
				{
					"supportsBackgroundPlayback": true,
					"supportsPictureInPicture": true
				}
			],
			[
				"expo-screen-orientation",
				{
					"initialOrientation": "DEFAULT"
				}
			],
			[
				"expo-font",
				{
					"fonts": [
						"node_modules/@expo-google-fonts/epilogue/Epilogue_400Regular.ttf",
						"node_modules/@expo-google-fonts/epilogue/Epilogue_500Medium.ttf",
						"node_modules/@expo-google-fonts/epilogue/Epilogue_800ExtraBold.ttf"
					]
				}
			],
			[
				"expo-splash-screen",
				{
					"backgroundColor": "#090607",
					"image": "./assets/splash-icon.png",
					"imageWidth": 200
				}
			],
			[
				"expo-av",
				{
					"microphonePermission": "Autoriser Lisa-Lou à accéder à votre microphone"
				}
			],
			[
				"expo-media-library",
				{
					"photosPermission": "Autoriser Lisa-Lou à accéder à vos photos.",
					"savePhotosPermission": "Autoriser Lisa-Lou à enregistrer vos photos.",
					"isAccessMediaLocationEnabled": false
				}
			],
			"expo-speech-recognition",
			"expo-localization"
		],
		"extra": {
			"eas": {
				"projectId": "5b858696-0448-4a8f-a91c-cbdaa34579a7"
			},
			"router": {
				"origin": "https://lisa-lou.expo.app"
			}
		},
		"owner": "lilykr"
	}
}
