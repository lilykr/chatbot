{
	"expo": {
		"name": "lisa-lou",
		"slug": "lisa-lou",
		"scheme": "lilykr.lisalou",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"newArchEnabled": true,
		"experiments": {
			"typedRoutes": true
		},
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.lilykr.lisalou",
			"backgroundColor": "#080607",
			"infoPlist": {
				"NSSpeechRecognitionUsageDescription": "Allow Lisa-Lou to use speech recognition to hear you and respond to you.",
				"NSMicrophoneUsageDescription": "Allow Lisa-Lou to use the microphone to hear you and respond to you.",
				"ITSAppUsesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#080607"
			},
			"package": "com.lilykr.lisalou",
			"permissions": [
				"android.permission.CAMERA",
				"android.permission.RECORD_AUDIO",
				"android.permission.MODIFY_AUDIO_SETTINGS",
				"android.permission.READ_EXTERNAL_STORAGE",
				"android.permission.WRITE_EXTERNAL_STORAGE",
				"android.permission.RECORD_AUDIO",
				"android.permission.MODIFY_AUDIO_SETTINGS",
				"android.permission.READ_EXTERNAL_STORAGE",
				"android.permission.WRITE_EXTERNAL_STORAGE"
			]
		},
		"androidStatusBar": {
			"translucent": true,
			"backgroundColor": "#090607"
		},
		"androidNavigationBar": {
			"barStyle": "dark-content",
			"backgroundColor": "#090607"
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"bundler": "metro",
			"output": "server"
		},
		"plugins": [
			[
				"expo-router",
				{
					"origin": "https://lisa-lou.expo.app"
				}
			],
			[
				"expo-screen-orientation",
				{
					"initialOrientation": "PORTRAIT_UP"
				}
			],
			[
				"expo-camera",
				{
					"cameraPermission": "Allow Lisa-Lou to access your camera to take photos and send them in the chat.",
					"microphonePermission": "Allow Lisa-Lou to access your microphone to hear you and respond to you.",
					"recordAudioAndroid": true
				}
			],
			[
				"expo-video",
				{
					"supportsBackgroundPlayback": false,
					"supportsPictureInPicture": false
				}
			],
			[
				"expo-screen-orientation",
				{
					"initialOrientation": "DEFAULT"
				}
			],
			[
				"expo-font",
				{
					"fonts": [
						"node_modules/@expo-google-fonts/epilogue/Epilogue_400Regular.ttf",
						"node_modules/@expo-google-fonts/epilogue/Epilogue_500Medium.ttf",
						"node_modules/@expo-google-fonts/epilogue/Epilogue_800ExtraBold.ttf"
					]
				}
			],
			[
				"expo-splash-screen",
				{
					"backgroundColor": "#090607",
					"image": "./assets/splash-icon.png",
					"imageWidth": 200
				}
			],
			[
				"expo-av",
				{
					"microphonePermission": "Allow Lisa-Lou to access your microphone to hear you and respond to you."
				}
			],
			[
				"expo-media-library",
				{
					"photosPermission": "Allow Lisa-Lou to access your photos to send them in the chat.",
					"savePhotosPermission": "Allow Lisa-Lou to save your photos to send them in the chat.",
					"isAccessMediaLocationEnabled": false
				}
			],
			[
				"expo-image-picker",
				{
					"photosPermission": "Allow Lisa-Lou to access your photos to send them in the chat."
				}
			],
			"expo-speech-recognition",
			"expo-localization"
		],
		"extra": {
			"eas": {
				"projectId": "5b858696-0448-4a8f-a91c-cbdaa34579a7"
			},
			"router": {
				"origin": "https://lisa-lou.expo.app"
			}
		},
		"owner": "lilykr",
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/5b858696-0448-4a8f-a91c-cbdaa34579a7"
		}
	}
}
